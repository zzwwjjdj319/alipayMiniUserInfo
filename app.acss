
.f-r{
  display: flex;
  flex-direction: row;
}
.f-c{
  display: flex;
  flex-direction: column;
}
.f-w{
  flex-wrap: wrap;
}
.j-a{
  justify-content: space-around;
}
.j-b{
  justify-content: space-between;
}
.j-c{
  justify-content: center;
}
.a-c{
  align-content: center;
  align-items: center;
}
.t-o{
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
page{
  background: linear-gradient(to right, #e3b7b7, #c4c4d1,#e3b7b7);
}

.ft-1{
